<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.su.booksale.mapper.ReturnRecordMapper">

    <select id="listReturnRecords" resultType="ReturnRecord">
        select * from returnstock order by returnTime DESC
    </select>

    <insert id="addReturnRecord" parameterType="ReturnRecord">
        insert into returnstock values (null,#{bookId},#{bookName},#{returnNum},#{returnTime,jdbcType=TIMESTAMP});
    </insert>

</mapper>