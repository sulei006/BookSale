<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.su.booksale.mapper.DaySellRecordMapper">

    <select id="listDaySellRecords" resultType="DaySellRecord">
        select * from daysellrecord order by tradeDate DESC
    </select>

    <insert id="addDaySellRecord" parameterType="DaySellRecord">
        insert into daysellrecord values (null,#{bookId},#{bookName},#{outPrice},#{outNumber},#{income},#{tradeDate,jdbcType=TIMESTAMP});
    </insert>

</mapper>